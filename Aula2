nota1 = input('Digite o primeiro nÃºmero:')
nota2 = input('Digite o segundo nÃºmero:')
nota3 = input('Digite o terceiro nÃºmero:')
nota4 = input('Digite o quarto nÃºmero:')

nota1 = int(nota1)
nota2 = int(nota2)
nota3 = int(nota3)
nota4 = int(nota4)

res = (nota1 +nota2 + nota3 + nota4)/4

if res>=6 and res<=10:
    print('Aluno aprovado')
